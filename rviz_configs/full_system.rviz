Panels:
  - Class: rviz_common/Displays
    Help Height: 78
    Name: Displays
    Property Tree Widget:
      Expanded:
        - /Global Options1
        - /Robot Models1
        - /Sensors1
        - /2D Coverage1
        - /SLAM 3D1
        - /Communication1
      Splitter Ratio: 0.5
    Tree Height: 600
  - Class: rviz_common/Selection
    Name: Selection
  - Class: rviz_common/Tool Properties
    Expanded:
      - /2D Pose Estimate1
      - /2D Goal Pose1
    Name: Tool Properties
    Splitter Ratio: 0.5886790156364441
  - Class: rviz_common/Views
    Expanded:
      - /Current View1
    Name: Views
    Splitter Ratio: 0.5
  - Class: rviz_common/Time
    Name: Time
    SyncMode: 0
    SyncSource: ""

Visualization Manager:
  Class: ""
  Displays:
    # Grid de base
    - Alpha: 0.3
      Cell Size: 1
      Class: rviz_default_plugins/Grid
      Color: 160; 160; 164
      Enabled: true
      Line Style:
        Line Width: 0.03
        Value: Lines
      Name: Grid
      Normal Cell Count: 0
      Offset:
        X: 0
        Y: 0
        Z: 0
      Plane: XY
      Plane Cell Count: 200
      Reference Frame: map
      Value: true
    
    # ============================================
    # ROBOT MODELS (UAVs)
    # ============================================
    - Class: rviz_default_plugins/Group
      Name: Robot Models
      Displays:
        - Alpha: 1
          Class: rviz_default_plugins/RobotModel
          Collision Enabled: false
          Description File: ""
          Description Source: Topic
          Description Topic:
            Depth: 5
            Durability Policy: Transient Local
            History Policy: Keep Last
            Reliability Policy: Reliable
            Value: /uav_0/robot_description
          Enabled: true
          Links:
            All Links Enabled: true
            Expand Joint Details: false
            Expand Link Details: false
            Expand Tree: false
            Link Tree Style: Links in Alphabetic Order
          Name: UAV 0
          TF Prefix: uav_0
          Update Interval: 0
          Value: true
          Visual Enabled: true
        
        - Alpha: 1
          Class: rviz_default_plugins/RobotModel
          Description Topic:
            Value: /uav_1/robot_description
          Enabled: true
          Name: UAV 1
          TF Prefix: uav_1
          Value: true
          Visual Enabled: true
        
        - Alpha: 1
          Class: rviz_default_plugins/RobotModel
          Description Topic:
            Value: /uav_2/robot_description
          Enabled: true
          Name: UAV 2
          TF Prefix: uav_2
          Value: true
          Visual Enabled: true
      Enabled: true
    
    # ============================================
    # SENSORS (LiDAR)
    # ============================================
    - Class: rviz_default_plugins/Group
      Name: Sensors
      Displays:
        - Alpha: 1.0
          Class: rviz_default_plugins/PointCloud2
          Color: 255; 255; 0
          Color Transformer: FlatColor
          Enabled: true
          Name: LiDAR UAV 0
          Size (m): 0.1
          Style: Points
          Topic:
            Value: /uav_0/velodyne_points
          Value: true
        
        - Alpha: 1.0
          Class: rviz_default_plugins/PointCloud2
          Color: 0; 255; 255
          Color Transformer: FlatColor
          Enabled: true
          Name: LiDAR UAV 1
          Size (m): 0.1
          Style: Points
          Topic:
            Value: /uav_1/velodyne_points
          Value: true
        
        - Alpha: 1.0
          Class: rviz_default_plugins/PointCloud2
          Color: 255; 0; 255
          Color Transformer: FlatColor
          Enabled: true
          Name: LiDAR UAV 2
          Size (m): 0.1
          Style: Points
          Topic:
            Value: /uav_2/velodyne_points
          Value: true
      Enabled: true
    
    # ============================================
    # 2D COVERAGE
    # ============================================
    - Class: rviz_default_plugins/Group
      Name: 2D Coverage
      Displays:
        # Global Occupancy Grid
        - Alpha: 0.7
          Class: rviz_default_plugins/Map
          Color Scheme: map
          Draw Behind: false
          Enabled: true
          Name: Global Occupancy
          Topic:
            Depth: 5
            Durability Policy: Volatile
            History Policy: Keep Last
            Reliability Policy: Reliable
            Value: /coverage_map_2d
          Update Topic:
            Depth: 5
            Durability Policy: Volatile
            History Policy: Keep Last
            Reliability Policy: Reliable
            Value: /coverage/global_map_updates
          Use Timestamp: false
          Value: true
        
        # Per-UAV Coverage (Colored)
        - Alpha: 0.9
          Class: rviz_default_plugins/GridCells
          Color: 255; 50; 50
          Enabled: true
          Name: Coverage UAV 0 (Red)
          Topic:
            Depth: 5
            Durability Policy: Volatile
            History Policy: Keep Last
            Reliability Policy: Reliable
            Value: /coverage/uav_0
          Value: true
        
        - Alpha: 0.9
          Class: rviz_default_plugins/GridCells
          Color: 50; 255; 50
          Enabled: true
          Name: Coverage UAV 1 (Green)
          Topic:
            Value: /coverage/uav_1
          Value: true
        
        - Alpha: 0.9
          Class: rviz_default_plugins/GridCells
          Color: 50; 50; 255
          Enabled: true
          Name: Coverage UAV 2 (Blue)
          Topic:
            Value: /coverage/uav_2
          Value: true
      Enabled: true
    
    # ============================================
    # SLAM 3D (Pointcloud en construction)
    # ============================================
    - Class: rviz_default_plugins/Group
      Name: SLAM 3D
      Displays:
        # SLAM Map UAV 0
        - Alpha: 1
          Autocompute Intensity Bounds: true
          Autocompute Value Bounds:
            Max Value: 10
            Min Value: -10
            Value: true
          Axis: Z
          Channel Name: intensity
          Class: rviz_default_plugins/PointCloud2
          Color: 255; 255; 0
          Color Transformer: FlatColor
          Decay Time: 0
          Enabled: true
          Invert Rainbow: false
          Max Color: 255; 255; 255
          Min Color: 0; 0; 0
          Name: SLAM Map UAV 0
          Position Transformer: XYZ
          Selectable: true
          Size (Pixels): 3
          Size (m): 0.08
          Style: Flat Squares
          Topic:
            Depth: 5
            Durability Policy: Volatile
            History Policy: Keep Last
            Reliability Policy: Best Effort
            Value: /uav_0/slam/map_points
          Use Fixed Frame: true
          Use rainbow: false
          Value: true
        
        # SLAM Map UAV 1
        - Alpha: 1
          Class: rviz_default_plugins/PointCloud2
          Color: 0; 255; 255
          Color Transformer: FlatColor
          Enabled: true
          Name: SLAM Map UAV 1
          Size (m): 0.08
          Style: Flat Squares
          Topic:
            Value: /uav_1/slam/map_points
          Use rainbow: false
          Value: true
        
        # SLAM Map UAV 2
        - Alpha: 1
          Class: rviz_default_plugins/PointCloud2
          Color: 255; 0; 255
          Color Transformer: FlatColor
          Enabled: true
          Name: SLAM Map UAV 2
          Size (m): 0.08
          Style: Flat Squares
          Topic:
            Value: /uav_2/slam/map_points
          Use rainbow: false
          Value: true
        
        # Global Merged Map (si disponible)
        - Alpha: 0.5
          Class: rviz_default_plugins/PointCloud2
          Color: 200; 200; 200
          Color Transformer: FlatColor
          Enabled: true
          Name: Merged Global Map
          Size (m): 0.05
          Style: Points
          Topic:
            Value: /slam/global_map
          Use rainbow: false
          Value: true
      Enabled: true
    
    # ============================================
    # COMMUNICATION GRAPH (30m range)
    # ============================================
    - Class: rviz_default_plugins/Group
      Name: Communication
      Displays:
        # Communication Links (Markers)
        - Class: rviz_default_plugins/MarkerArray
          Enabled: true
          Name: Comm Links (30m)
          Namespaces:
            {}
          Topic:
            Depth: 5
            Durability Policy: Volatile
            History Policy: Keep Last
            Reliability Policy: Reliable
            Value: /communication/graph
          Value: true
        
        # Communication Range Circles
        - Class: rviz_default_plugins/Marker
          Enabled: true
          Name: Comm Range Visualization
          Namespaces:
            {}
          Topic:
            Depth: 5
            Durability Policy: Volatile
            History Policy: Keep Last
            Reliability Policy: Reliable
            Value: /communication/range_markers
          Value: true
      Enabled: false  # Désactivé par défaut (activer si GNN utilisé)
    
    # ============================================
    # TF FRAMES (UAVs)
    # ============================================
    - Class: rviz_default_plugins/TF
      Enabled: true
      Frame Timeout: 15
      Frames:
        All Enabled: false
        map:
          Value: true
        uav_0:
          Value: true
        uav_0/base_link:
          Value: true
        uav_0/lidar_link:
          Value: true
        uav_1:
          Value: true
        uav_1/base_link:
          Value: true
        uav_1/lidar_link:
          Value: true
        uav_2:
          Value: true
        uav_2/base_link:
          Value: true
        uav_2/lidar_link:
          Value: true
      Marker Scale: 1
      Name: TF
      Show Arrows: true
      Show Axes: true
      Show Names: true
      Tree:
        map:
          uav_0:
            uav_0/base_link:
              uav_0/lidar_link:
                {}
          uav_1:
            uav_1/base_link:
              uav_1/lidar_link:
                {}
          uav_2:
            uav_2/base_link:
              uav_2/lidar_link:
                {}
      Update Interval: 0
      Value: true
    
    # ============================================
    # TRAJECTOIRES (Odometry Path)
    # ============================================
    - Class: rviz_default_plugins/Group
      Name: Trajectoires
      Displays:
        - Alpha: 1
          Buffer Length: 100
          Class: rviz_default_plugins/Path
          Color: 255; 85; 255
          Enabled: true
          Head Diameter: 0.3
          Head Length: 0.2
          Length: 0.3
          Line Style: Lines
          Line Width: 0.05
          Name: Path UAV 0
          Offset:
            X: 0
            Y: 0
            Z: 0
          Pose Color: 255; 85; 255
          Pose Style: None
          Radius: 0.03
          Shaft Diameter: 0.1
          Shaft Length: 0.1
          Topic:
            Depth: 5
            Durability Policy: Volatile
            History Policy: Keep Last
            Reliability Policy: Reliable
            Value: /uav_0/path
          Value: true
        
        - Alpha: 1
          Buffer Length: 100
          Class: rviz_default_plugins/Path
          Color: 85; 255; 127
          Enabled: true
          Line Style: Lines
          Line Width: 0.05
          Name: Path UAV 1
          Topic:
            Value: /uav_1/path
          Value: true
        
        - Alpha: 1
          Buffer Length: 100
          Class: rviz_default_plugins/Path
          Color: 127; 85; 255
          Enabled: true
          Line Style: Lines
          Line Width: 0.05
          Name: Path UAV 2
          Topic:
            Value: /uav_2/path
          Value: true
      Enabled: true
  
  Enabled: true
  Global Options:
    Background Color: 48; 48; 48
    Fixed Frame: map
    Frame Rate: 30
  Name: root
  Tools:
    - Class: rviz_default_plugins/Interact
      Hide Inactive Objects: true
    - Class: rviz_default_plugins/MoveCamera
    - Class: rviz_default_plugins/Select
    - Class: rviz_default_plugins/FocusCamera
    - Class: rviz_default_plugins/Measure
      Line color: 128; 128; 0
    - Class: rviz_default_plugins/SetInitialPose
      Topic:
        Depth: 5
        Durability Policy: Volatile
        History Policy: Keep Last
        Reliability Policy: Reliable
        Value: /initialpose
    - Class: rviz_default_plugins/SetGoal
      Topic:
        Depth: 5
        Durability Policy: Volatile
        History Policy: Keep Last
        Reliability Policy: Reliable
        Value: /goal_pose
    - Class: rviz_default_plugins/PublishPoint
      Single click: true
      Topic:
        Depth: 5
        Durability Policy: Volatile
        History Policy: Keep Last
        Reliability Policy: Reliable
        Value: /clicked_point
  Transformation:
    Current:
      Class: rviz_default_plugins/TF
  Value: true
  Views:
    Current:
      Class: rviz_default_plugins/Orbit
      Distance: 60
      Enable Stereo Rendering:
        Stereo Eye Separation: 0.06
        Stereo Focal Distance: 1
        Swap Stereo Eyes: false
        Value: false
      Focal Point:
        X: 0
        Y: 0
        Z: 0
      Focal Shape Fixed Size: true
      Focal Shape Size: 0.05
      Invert Z Axis: false
      Name: Current View
      Near Clip Distance: 0.01
      Pitch: 0.785398
      Target Frame: <Fixed Frame>
      Value: Orbit (rviz)
      Yaw: 0.785398
    Saved: ~

